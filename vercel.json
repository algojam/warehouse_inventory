{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    // Tiyakin po na wala na ang "DATABASE_URL": "@DATABASE_URL" dito
    // Kung walang ibang env variable, dapat ay walang laman ang "env": {}
  }
}